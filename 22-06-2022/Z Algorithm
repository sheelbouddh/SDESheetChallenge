public class Solution {

	public static int zAlgorithm(String s, String p, int n, int m) {
		if(m > n) return -1;
        
        int ans = 0;
        for(int i = 0; i < n-m+1; i++){
            if(s.substring(i, i+m).equals(p)){
                ans++;
            }
        }
        
        return ans;

	}

}
